/*문제: 리포트표지를 출력하는 프로그램을 작성하여보자.
* ****************************************
*                 리포트
* ****************************************
* 
* ****************************************
*                       학과:컴퓨터 공학과
*                       학번:2018001
*                       성명:홍길동
* ****************************************
*/
 
#include <stdio.h>

int main()
{
	for (int i = 0; i < 50; i++)
	{
		printf("*");
	}
	printf("\n\t\t\t리포트\n");
	for (int i = 0; i < 50; i++)
	{
		printf("*");
	}
	printf("\n\t\t\t\t학과:컴퓨터 공학과\n");
	printf("\t\t\t\t학번: 2018001\n");
	printf("\t\t\t\t성명: 홍길동\n");
	return 0;
}
