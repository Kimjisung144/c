#include <stdio.h>

int main() {
	int a,b;
	printf("시간과 나이를 입력하세요");
	scanf_s("%d %d", &a, &b);
	if (a <= 17)
	{
		if (b >= 65 || b <= 12)
		{
			printf("25,000원입니다.");
		}
		else
		{
			printf("34,000원입니다.");
		}
	}
	else if (a > 17 && a<24)
	{
		printf("10,000원입니다.");
	}
	else
	{
		printf("잘못입력하셨습니다.");
	}
	return 0;
}
