#include <stdio.h>

int main() {
	int a, b, c;
	int max;
	scanf("%d %d %d", &a, &b, &c);
	if (a >= b)
	{
		max = a;
		if (max < c)
		{
			max = c;
		}
		else 
		{
			max = max;
		}
	}
	else if (a < b)
	{
		max = b;
		if (max < c)
		{
			max = c;
		}
		else
		{
			max = max;
		}
	}
	else
	{
		printf("잘못되었습니다.");
	}
	printf("%d", max);
	return 0;
}
