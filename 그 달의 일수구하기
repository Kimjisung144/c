#include <stdio.h>

int main() {
	int a;
	scanf_s("%d", &a);
	if (a == 2)
	{
		printf("%d은 28일입니다.",a);
	}
	else if (a == 4 || a==6 || a == 9 || a == 11)
	{
		printf("%d은 30일 입니다.",a);
	}
	else if (a == 1 || a == 3 || a == 5 || a == 7 ||a == 8 || a == 10 || a == 12 )
	{
		printf("%d은 31일 입니다",a);
	}
	return 0;
}
