#include <stdio.h>

int main() {
	int a,b,c;
	printf("좌표(x,y)입력하시오:");
	scanf_s("%d %d",&a,&b);
	if (a > 0 && b > 0){
		c = 1;
	}
	else if (a > 0 && b < 0){
		c = 2;
	}
	else if (a < 0 && b>0) {
		c = 3;
	}
	else if (a < 0 && b < 0){
		c = 4;
	}

	switch(c)
	{
		case 1:
			printf("1사분면입니다.");
			break;
		case 2:
			printf("4사분면입니다.");
			break;
		case 3:
			printf("2사분면입니다.");
			break;
		case 4:
			printf("3사분면입니다.");
			break;
	}
	return 0;
}
