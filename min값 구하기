#include <stdio.h>

int main() {
	int a, b, c;
	int min;
	scanf("%d %d %d", &a, &b, &c);
	if (a >= b)
	{
		min = b;
		if (min> c)
		{
			min = c;
		}
		else
		{
			min =min;
		}
	}
	else if (a < b)
	{
		min = a;
		if (min > c)
		{
			min = c;
		}
		else
		{
			min = min;
		}
	}
	else
	{
		printf("잘못되었습니다.");
	}
	printf("%d", min);
	return 0;
}
