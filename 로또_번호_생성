#include <stdio.h>
#include <stdlib.h> //srand, rand를 사용하기 위한 헤더파일
#include <time.h> // time을 사용하기 위한 헤더파일

int main()
{
	int list[6];//6개 저장
	int bin = 0;; //리스트 위치 바꾸기 위한 변수
	int count = 0;
	srand(time(NULL)); // 난수 초기화
	for (int i = 0; i < 6; i++) // 10회 반복
	{
		goal:
		list[i] = rand() % 46; // 0 ~ 45 사이의 숫자를 뽑아서 random 변수에 저장
		if (list[i] == 0)//다시 랜덤값 받기 위해 가기
		{
			goto goal;  // 만약 랜덤값이 0나오면 1로 바꾸기
			break;	
		}
		for (int j = 0; j < i; j++)
		{
			if (list[i] == list[j]) // 값은 번호가 나오면 안되기에 방지하는 if문
			{
				goto goal; // 다시 돌아가
				break;
			}
		}
	}
	for (int z = 0; z < 6; z++)
	{
		count = 0;
		for (int a = 0; a < 6; a++)
		{
			if (list[z + count] > list[a])
			{
				bin = list[z + count];
				list[z + count] = list[a];
				list[a] = bin;
				bin = 0;
				count++;
			}
		}
	}
	printf("결과 값은:");
	for (int m = 0; m < 6; m++)
	{
		printf("%d\t", list[m]);
	}
	return 0;
}
